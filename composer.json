{
    "name": "edujugon/laravel-google-ads",
    "description": "Laravel package for Google Adwords. Easy to use wrapper of googleads/googleads-php-lib for Laravel",
    "keywords": ["google","adwords","ads","laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Eduardo Marcos",
            "email": "edujugon@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.4",
        "googleads/googleads-php-lib": "^25.1"
    },
    "require-dev": {
      "phpunit/phpunit": "~5.0"
    },
    "autoload": {
      "psr-4": {
        "Edujugon\\GoogleAds\\": "src/"
      },
      "files": [
          "src/Support/helpers.php"
      ]
    },
    "minimum-stability": "dev"
}
